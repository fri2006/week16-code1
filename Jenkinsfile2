pipeline {
    agent any
    tools {
        maven 'M2_HOME'
    }
     stage('sonarqube scan'){
    steps{
        sh 'mvn verify org.sonarsource.scanner.maven:sonar-maven-plugin:sonar -Dsonar.projectKey=fri2006_week16-code1'
    }
}
        stage('all maven commands'){
           steps{
            sh 'mvn clean test compile install package'
           }   
        }
}
